    "            browser = await p.chromium.launch(\n",
    "                headless=True,\n",
    "                proxy=PROXY_CONFIG_PLAYWRIGHT,\n",
    "                timeout=30000\n",
    "            )\n",
    "\n",
    "            page = await browser.new_page()\n",
    "            await page.goto(IP_CHECK_URL, wait_until=\"load\", timeout=30000)\n",
