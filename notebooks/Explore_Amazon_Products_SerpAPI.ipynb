    "\n",
    "import nest_asyncio\n",
    "nest_asyncio.apply()\n",
    "\n",
    "from playwright.async_api import async_playwright\n",
    "\n",
    "async def get_sellers_from_asin(asin: str):\n",
    "    async with async_playwright() as p:\n",
    "        browser = await p.chromium.launch(headless=True)\n",
    "        page = await browser.new_page()\n",
    "\n",
    "        # Offer listing page (all sellers)\n",
    "        url = f\"https://www.amazon.de/gp/offer-listing/{asin}/\"\n",
    "        await page.goto(url, timeout=60000)\n",
    "\n",
    "        sellers = []\n",
    "\n",
    "        # Find all links with seller IDs\n",
    "        elements = page.locator(\"a[href*='seller=']\")\n",
    "        count = await elements.count()\n",
    "        for i in range(count):\n",
    "            href = await elements.nth(i).get_attribute(\"href\")\n",
    "            text = await elements.nth(i).inner_text()\n",
    "            if href and \"seller=\" in href:\n",
    "                seller_id = href.split(\"seller=\")[-1].split(\"&\")[0]\n",
    "                seller_name = text.strip()\n",
    "                sellers.append({\"asin\": asin, \"seller_name\": seller_name, \"seller_id\": seller_id})\n",
    "\n",
    "        # Fallback: if no seller found, assume Amazon Retail\n",
    "        if not sellers:\n",
    "            sellers.append({\"asin\": asin, \"seller_name\": \"Amazon.de\", \"seller_id\": \"AMAZON_RETAIL\"})\n",
    "\n",
    "        await browser.close()\n",
    "        return sellers\n",
    "\n",
    "# ‚úÖ Example usage for your Yucateco Sauce\n",
    "asin = \"B0199AMFJ0\"\n",
    "sellers = await get_sellers_from_asin(asin)\n",
    "for s in sellers:\n",
    "    print(s)\n"
   ]
