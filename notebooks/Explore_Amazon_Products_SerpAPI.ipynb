    "# ------------------------------------------------------------------\n",
    "# üõë RERUN SIMPLIFIED AMAZON CONNECTION TEST\n",
    "# ------------------------------------------------------------------\n",
    "import nest_asyncio\n",
    "nest_asyncio.apply()\n",
    "import asyncio\n",
    "from playwright.async_api import async_playwright\n",
    "\n",
    "async def simple_amazon_check(asin: str):\n",
    "    url = f\"https://www.amazon.de/gp/offer-listing/{asin}/\"\n",
    "\n",
    "    async with async_playwright() as p:\n",
    "        try:\n",
    "            # Uses the newly updated PROXY_CONFIG_PLAYWRIGHT with the fresh IP\n",
    "            browser = await p.chromium.launch(\n",
    "                headless=True,\n",
    "                proxy=PROXY_CONFIG_PLAYWRIGHT,\n",
    "                timeout=60000\n",
    "            )\n",
    "\n",
    "            page = await browser.new_page()\n",
    "\n",
    "            print(f\"Navigating to {url} via proxy...\")\n",
    "\n",
    "            # Navigate with extended goto timeout (90s)\n",
    "            await page.goto(url, timeout=90000)\n",
    "\n",
    "            title = await page.title()\n",
    "\n",
    "            # Wait for the main offer container (30s)\n",
    "            await page.wait_for_selector(\n",
    "                \"div.olpOffer\",\n",
    "                timeout=30000\n",
    "            )\n",
    "\n",
    "            await browser.close()\n",
    "\n",
    "            print(\"------------------------------------------\")\n",
    "            print(\"‚úÖ SUCCESS! Amazon page fully loaded via proxy.\")\n",
    "            print(f\"   Page Title: {title}\")\n",
    "            print(\"------------------------------------------\")\n",
    "            return True\n",
    "\n",
    "        except Exception as e:\n",
    "            print(\"------------------------------------------\")\n",
    "            print(f\"‚ùå CONNECTION FAILED: Amazon Anti-Bot Detected or Timeout.\")\n",
    "            print(f\"   Error: {e}\")\n",
    "            print(\"   Action: You need to rotate your proxy IP again (change the session ID).\")\n",
    "            print(\"------------------------------------------\")\n",
    "            try:\n",
    "                await browser.close()\n",
    "            except:\n",
    "                pass\n",
    "            return False\n",
    "\n",
    "# ------------------------------------------------------------------\n",
    "# üõë EXECUTION BLOCK\n",
    "# ------------------------------------------------------------------\n",
    "asin = \"B0199AMFJ0\"\n",
    "await simple_amazon_check(asin)"
