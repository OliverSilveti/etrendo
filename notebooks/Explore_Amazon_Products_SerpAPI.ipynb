    "import pandas as pd\n",
    "\n",
    "products = []\n",
    "\n",
    "for page_index, page in enumerate(data, start=1):  # enumerate to track page number\n",
    "    organic = page.get(\"organic_results\", [])\n",
    "    for p in organic:\n",
    "        # Handle price safely\n",
    "        price_raw, currency = None, None\n",
    "        price = p.get(\"price\")\n",
    "        if isinstance(price, dict):\n",
    "            price_raw = price.get(\"raw\")\n",
    "            currency = price.get(\"currency\")\n",
    "        elif isinstance(price, str):\n",
    "            price_raw = price\n",
    "\n",
    "        products.append({\n",
    "            \"page_number\": page_index,                     # ‚úÖ new\n",
    "            \"category_label\": CONFIG[\"category_label\"],     # from crawl\n",
    "            \"node\": page.get(\"search_parameters\", {}).get(\"node\"),  # ‚úÖ new\n",
    "\n",
    "            \"position\": p.get(\"position\"),\n",
    "            \"asin\": p.get(\"asin\"),\n",
    "            \"title\": p.get(\"title\"),\n",
    "            \"link\": p.get(\"link\"),\n",
    "            \"link_clean\": p.get(\"link_clean\"),\n",
    "            \"thumbnail\": p.get(\"thumbnail\"),\n",
    "            \"rating\": p.get(\"rating\"),\n",
    "            \"reviews\": p.get(\"reviews\"),\n",
    "            \"bought_last_month\": p.get(\"bought_last_month\"),\n",
    "            \"price_raw\": price_raw,\n",
    "            \"currency\": currency,\n",
    "            \"extracted_price\": p.get(\"extracted_price\"),\n",
    "            \"offers\": p.get(\"offers\"),\n",
    "            \"delivery\": p.get(\"delivery\"),\n",
    "            \"climate_pledge_friendly\": p.get(\"climate_pledge_friendly\"),\n",
    "            \"sustainability_features\": p.get(\"sustainability_features\"),\n",
    "            \"is_sponsored\": p.get(\"is_sponsored\", False),\n",
    "            \"extracted_at\": p.get(\"extracted_at\")\n",
    "        })\n",
    "\n",
    "df = pd.DataFrame(products)\n",
    "print(f\"‚úÖ Normalized {len(df)} products into DataFrame with {len(df.columns)} columns\")\n",
    "\n",
    "df.head()"
   ]
